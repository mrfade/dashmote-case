{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./src/*"],
      "~~/*": ["./*"],
      "@@/*": ["./src/*"]
    }
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true,
  },
  "exclude": ["node_modules", "dist"]
}